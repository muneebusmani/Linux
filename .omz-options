# Post Plugin options
export NVM_LAZY_LOAD=true
export ZSH_TMUX_AUTOSTART=true
export ZSH_TMUX_AUTOQUIT=false
export VI_MODE_RESET_PROMPT_ON_MODE_CHANGE=true
